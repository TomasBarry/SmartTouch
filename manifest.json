{
  "manifest_version": 2,

  "name": "literate-train",
  "description": "This extension will refresh the page when a user swipes",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [
              "/js/external/jquery.js",
              "/js/content_script.js"
            ]
      }
    ]
}